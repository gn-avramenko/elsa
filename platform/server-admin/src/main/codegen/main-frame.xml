<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://vga.com/elsa/meta-web-app">
    <entity id="com.gridnine.platform.elsa.admin.web.mainFrame.WorkspaceItem">
        <property id="name" type="STRING" non-nullable="true"/>
        <property id="icon" type="STRING" />
        <property id="link" type="STRING" non-nullable="true"/>
    </entity>
    <entity id="com.gridnine.platform.elsa.admin.web.mainFrame.WorkspaceGroup">
        <property id="name" type="STRING" non-nullable="true"/>
        <property id="icon" type="STRING"/>
        <collection id="items" element-type="ENTITY" element-class-name="com.gridnine.platform.elsa.admin.web.mainFrame.WorkspaceItem"/>
    </entity>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.mainFrame.MainFrameDialogButton" managed-configuration="true">
        <server-managed-state>
            <property id="title" type="STRING"/>
            <property id="disabled" type="BOOLEAN" non-nullable="true"/>
            <property id="hidden" type="BOOLEAN" non-nullable="true"/>
            <property id="buttonType" type="STRING"/>
            <property id="color" type="STRING"/>
            <property id="icon" type="STRING"/>
        </server-managed-state>
        <commands-from-client>
            <command id="click"/>
        </commands-from-client>
    </custom-container>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.mainFrame.MainFrameDialogFooter"/>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.mainFrame.MainFrameDialogHeader"/>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.mainFrame.MainFrame">
        <server-managed-state>
            <collection id="workspaceGroups" element-type="ENTITY" element-class-name="com.gridnine.platform.elsa.admin.web.mainFrame.WorkspaceGroup"/>
            <property id="title" type="STRING"/>
            <property id="appName" type="STRING"/>
            <property id="backUrl" type="STRING"/>
            <property id="embeddedMode" type="BOOLEAN"/>
            <property id="themeToken" type="STRING" non-nullable="true"/>
        </server-managed-state>
        <commands-from-server>
            <command id="setWebPeerParam">
                <property id="key" type="STRING" non-nullable="true"/>
                <property id="value" type="STRING" non-nullable="true"/>
            </command>
            <command id="reloadPage"/>
            <command id="saveFile">
                <property id="name" type="STRING" non-nullable="true"/>
                <property id="base64Content" type="STRING" non-nullable="true"/>
            </command>
            <command id="showConfirmationDialog">
                <property id="title" type="STRING" non-nullable="true"/>
                <property id="okText" type="STRING" non-nullable="true"/>
                <property id="cancelText" type="STRING" non-nullable="true"/>
                <property id="question" type="STRING" non-nullable="true"/>
            </command>
            <command id="showError">
                <property id="title" type="STRING" non-nullable="true"/>
                <property id="closeText" type="STRING" non-nullable="true"/>
                <property id="error" type="STRING" non-nullable="true"/>
            </command>
            <command id="showDialogInternal"/>
            <command id="closeDialogInternal"/>
        </commands-from-server>
        <commands-from-client>
            <command id="processConfirmationResult">
                <property id="okPressed" type="BOOLEAN" non-nullable="true"/>
            </command>
        </commands-from-client>
        <children>
            <router id="mainRouter" class-name="com.gridnine.platform.elsa.admin.web.mainFrame.MainRouter"/>
        </children>
    </custom-container>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.mainFrame.HomePage"/>
</web-app>