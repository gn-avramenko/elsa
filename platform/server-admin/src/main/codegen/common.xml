<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://vga.com/elsa/meta-web-app">
    <enum id="com.gridnine.platform.elsa.admin.web.common.BreakPoint">
        <enum-item id="MOBILE"/>
        <enum-item id="DESKTOP"/>
    </enum>
    <button class-name="com.gridnine.platform.elsa.admin.web.common.Button">
        <server-managed-state>
            <property id="tooltip" type="STRING"/>
        </server-managed-state>
    </button>
    <entity id="com.gridnine.platform.elsa.admin.web.common.Option">
        <property id="id" type="STRING" non-nullable="true"/>
        <property id="displayName" type="STRING" non-nullable="true"/>
    </entity>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.common.ContentWrapper" managed-configuration="true"/>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.common.Glue"/>
    <entity id="com.gridnine.platform.elsa.admin.web.common.IconMenuItem">
        <property id="id" type="STRING" non-nullable="true"/>
        <property id="icon" type="STRING" non-nullable="true"/>
        <property id="displayName" type="STRING" non-nullable="true"/>
    </entity>
    <custom class-name="com.gridnine.platform.elsa.admin.web.common.DropDownIcon">
        <server-managed-state>
            <collection id="items" element-type="ENTITY" element-class-name="com.gridnine.platform.elsa.admin.web.common.IconMenuItem"/>
            <property id="selectedItem" type="ENTITY" class-name="com.gridnine.platform.elsa.admin.web.common.IconMenuItem" non-nullable="true"/>
        </server-managed-state>
        <commands-from-client>
            <command id="click">
                <property id="id" type="STRING" non-nullable="true"/>
            </command>
        </commands-from-client>
    </custom>
    <entity id="com.gridnine.platform.elsa.admin.web.common.ImageMenuItem">
        <property id="id" type="STRING" non-nullable="true"/>
        <property id="src" type="STRING" non-nullable="true"/>
        <property id="displayName" type="STRING"/>
        <property id="imageWidth" type="INT" />
        <property id="imageHeight" type="INT" />
    </entity>
    <custom class-name="com.gridnine.platform.elsa.admin.web.common.DropDownImage">
        <server-managed-state>
            <collection id="items" element-type="ENTITY" element-class-name="com.gridnine.platform.elsa.admin.web.common.ImageMenuItem"/>
            <property id="selectedItem" type="ENTITY" class-name="com.gridnine.platform.elsa.admin.web.common.ImageMenuItem" non-nullable="true"/>
        </server-managed-state>
        <commands-from-client>
            <command id="click">
                <property id="id" type="STRING" non-nullable="true"/>
            </command>
        </commands-from-client>
    </custom>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.dialog.LabelHeader">
        <server-managed-state>
            <property id="title" type="STRING" non-nullable="true"/>
        </server-managed-state>
    </custom-container>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.common.GroupEditor">
        <commands-from-client>
            <command id="add">
                <property id="idx" type="INT"/>
            </command>
            <command id="delete">
                <property id="idx" type="INT"/>
            </command>
            <command id="moveUp">
                <property id="idx" type="INT"/>
            </command>
            <command id="moveDown">
                <property id="idx" type="INT"/>
            </command>
        </commands-from-client>
    </custom-container>
    <enum id="com.gridnine.platform.elsa.admin.web.common.RestrictionType">
        <enum-item id="SIMPLE"/>
        <enum-item id="OR"/>
        <enum-item id="AND"/>
        <enum-item id="NOT"/>
    </enum>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.common.RestrictionEditor">
        <server-managed-state>
            <property id="restrictionType" type="ENUM" class-name="com.gridnine.platform.elsa.admin.web.common.RestrictionType"/>
        </server-managed-state>
    </custom-container>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.common.RestrictionsEditor">
        <server-managed-state>
            <property id="l10nAnd" type="STRING" non-nullable="true"/>
            <property id="l10nOr" type="STRING" non-nullable="true"/>
            <property id="l10nNot" type="STRING" non-nullable="true"/>
            <property id="l10nSimple" type="STRING" non-nullable="true"/>
        </server-managed-state>
        <commands-from-client>
            <command id="add">
                <property id="elementId" type="STRING"/>
                <property id="restrictionType" type="STRING"/>
            </command>
            <command id="delete">
                <property id="elementId" type="STRING"/>
            </command>
            <command id="moveUp">
                <property id="elementId" type="STRING"/>
            </command>
            <command id="moveDown">
                <property id="elementId" type="STRING"/>
            </command>
        </commands-from-client>
    </custom-container>
    <custom-container class-name="com.gridnine.platform.elsa.admin.web.common.ActionsEditor">
        <commands-from-client>
            <command id="add">
                <property id="idx" type="INT"/>
            </command>
            <command id="delete">
                <property id="idx" type="INT"/>
            </command>
            <command id="moveUp">
                <property id="idx" type="INT"/>
            </command>
            <command id="moveDown">
                <property id="idx" type="INT"/>
            </command>
        </commands-from-client>
    </custom-container>
</web-app>
